package fundamentos.operadores;

import java.util.Scanner;

public class DesafioQuadradoCuco {

	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		System.out.println("Informe o valor para ser elevado ao Quadrado e ao Cubo: ");
		
		
		Double quadrado = Math.pow(entrada.nextDouble(), 2);
		Double cubo = Math.pow(entrada.nextDouble(), 3);
		
		System.out.printf("O valor ",entrada,quadrado,cubo);
		entrada.close();
	}
}
