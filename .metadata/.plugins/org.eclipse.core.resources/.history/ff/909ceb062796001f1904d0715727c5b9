package controle;

import java.util.Scanner;

public class DoWhile {

	public static void main(String[] args) {
		
		// if (...) sentenca; ou {}
		// while (...) sentenca; ou {}
		// for (...;...;...) sentenca; ou {}
		
		//do sentenca; ou {} while (...);
		
		Scanner entrada = new Scanner(System.in);
		
		do {
			
		} while (false) {
			
		}

		entrada.close();
	}
}
